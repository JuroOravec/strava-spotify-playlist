    "passport-strava-oauth2": "^0.1.3",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "pg-pool": "^3.2.2",
    "pino": "^6.8.0",
    "spotify-web-api-node": "^5.0.0",
    "strava-v3": "^2.0.9",
    "tmp": "^0.2.1",
