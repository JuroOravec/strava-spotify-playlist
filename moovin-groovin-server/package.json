{
  "name": "moovin-groovin-server",
  "version": "1.0.0",
  "description": "Server for integrating Strava API with Spotify API",
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com> (https://github.com/jurooravec)",
  "main": "index.ts",
  "scripts": {
    "start": "NODE_ENV=production STRAVA_VERIFY_TOKEN_SEED=$(date +%s) node dist/index.js",
    "build": "ts-node build/beforeBuild.ts && tsc && ts-node build/afterBuild.ts",
    "dev": "NODE_ENV=development nodemon src/index.ts | pino-pretty",
    "dev:debug": "NODE_ENV=development nodemon --exec \"node -r ts-node/register --inspect-brk\" src/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{src, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{src, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "typegen": "graphql-codegen -- --config codegen.yml"
  },
  "dependencies": {
    "@types/better-queue": "^3.8.2",
    "@types/connect-pg-simple": "^4.2.2",
    "@types/express-session": "^1.17.3",
