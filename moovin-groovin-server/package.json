    "@types/lodash": "^4.14.165",
    "@types/passport": "^1.0.5",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-spotify": "^1.1.0",
    "@types/pg": "^7.14.7",
    "@types/pg-format": "^1.0.1",
    "@types/pg-pool": "^2.0.2",
    "@types/spotify-web-api-node": "^4.0.2",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "apollo-server-express": "^2.19.2",
    "async-exit-hook": "^2.0.1",
    "better-queue": "^3.8.10",
    "body-parser": "^1.19.0",
    "connect-pg-simple": "^6.2.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.9.2",
    "express-session": "^1.17.1",
    "express-slash": "^2.0.1",
    "graphql": "^15.4.0",
    "handlebars": "^4.7.6",
    "install": "^0.13.0",
